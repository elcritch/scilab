{ 
	"_metadata_": { 
		"_kind_": "instron", 
		"_protocol_": "fatigue cycle failure instron data and normalization"
	},
	"_stages_": [
		{
			"_name_": "raw",
			"_inputs_": { "csv": "_file_" },
			"_outputs_": { },
			"_sources_": {
				"preload_csv": {
					"trends":    { "_csv_": "raw/01 Preloads/*.trends.csv" },
					"tracking":  { "_csv_": "raw/01 Preloads/*.tracking.csv" }
				},
				"precond_csv": {
					"trends":    { "_csv_": "raw/02 Preconditions/*.trends.csv" },
					"tracking":  { "_csv_": "raw/02 Preconditions/*.tracking.csv" }
				},
				"cycles_lg_csv": { 
					"trends":    { "_csv_": "raw/03 Fatigue Cycles (LG)/*.trends.csv" },
					"tracking":  { "_csv_": "raw/03 Fatigue Cycles (LG)/*.tracking.csv" }
				 },
				"cycles_tr_csv": { 
					"trends":    { "_csv_": "raw/03 Fatigue Cycles (TR)/*.trends.csv" },
					"tracking":  { "_csv_": "raw/03 Fatigue Cycles (TR)/*.tracking.csv" }
				 }
			},
			"_computations_": {}
		},
		{
			"_name_": "pre",
			"_inputs_": { "raw": "_stage_" },
			"_outputs_": [ "matlab", "excel" ],
			"_sources_": {
				"preload": {
					"trends":   { "_field_": "raw.preconds_csv.trends" },
					"tracking": { "_field_": "raw.preconds_csv." }
				},
				"precond": {
					"trends":   { "_field_": "raw.preload_csv" },
					"tracking": { "_field_": "raw.preload_csv" }
				},
				"fatigue": {
					"trends":   { "_lookup_": { "details.info.orientation": {"tr": "raw.cycles_tr_csv", "lg": "raw.cycles_lg_csv"} } },
					"tracking": { "_lookup_": { "details.info.orientation": {"tr": "raw.cycles_tr_csv", "lg": "raw.cycles_lg_csv"} } }
				}
			},
			"_computations_": {
				"trends":   ["columns.trends.pre"],
				"tracking": ["columns.tracking.pre"]
			}
		},
		{
			"_name_": "norm",
			"_inputs_": {"pre": "_stage_" },
			"_outputs_": [ "matlab", "excel" ],
			"_sources_": {
		        "preload":  { 
					"trends":   { "_field_": "pre.preload_csv" },
					"tracking": { "_field_": "pre.preload_csv" }
				},
		        "precond":  {
					"trends":   { "_field_": "pre.preconds_csv" },
					"tracking": { "_field_": "pre.preconds_csv" }
				},
		        "fatigue":  {
					"trends":   { "_lookup_": {"details.info.orientation": { "tr": "raw.cycles_tr_csv", "lg": "raw.cycles_lg_csv"} } },
					"tracking": { "_lookup_": {"details.info.orientation": { "tr": "raw.cycles_tr_csv", "lg": "raw.cycles_lg_csv"} } }
				}
			},
			"_computations_": {
				"trends":   ["_computations_.columns.trends.norm"],
				"tracking": ["_computations_.columns.tracking.norm"]
			}	
		}
	],
	"_computations_": {
		"summaries": {
			
		},
		"columns": {
			"tracking": {
				"pre": [
		            { "name": "step",                   "label": "Step",                    "units": "Nº",  "full": "Step"                                       },
		            { "name": "totalTime",              "label": "Total Time",              "units": "s",   "full": "Total Time (s)"                             },
		            { "name": "disp",                   "label": "Displacement",            "units": "mm",  "full": "Displacement(Linear|Digital Position) (mm)" },
		            { "name": "load_1kN",               "label": "Load (1kN)",              "units": "N",   "full": "Load(Linear|Load) (N)"                      },
		            { "name": "load_missus",            "label": "Load (Missus)",           "units": "N",   "full": "Load(Linear|Load1) (N)"                     },
		            { "name": "elapsedCycles",          "label": "Elapsed Cycles",          "units": "Nº",  "full": "Elapsed Cycles"                             },
		            { "name": "cycleElapsedTime",       "label": "Cycle Elapsed Time ",     "units": "s",   "full": "Cycle Elapsed Time (s)"                     },
		            { "name": "totalCycleCount",        "label": "Total Cycle Count",       "units": "Nº",  "full": "Total Cycle Count(Linear Waveform)"         }
				],
				"norm": [
					{
						"column":       { "name":  "totalTime", "units": "s", "label": "Total Time" },
						"_source_":     { "__field__": "data.raw.totalTime" }, 
						"_conversion_": {}
					},
					{ 
						"column":       { "name":  "elapsedCycles", "units": "Nº:Cycles/Step", "label": "Elapsed Cycles" },
						"_source_":     { "__field__": "data.raw.elapsedCycles" }, 
						"_conversion_": {}
					},
					{
						"column":       { "name":  "totalCycleCount", "units": "Nº:Cycles/Test", "label": "Total Cycle Count" },
						"_source_":     { "__field__": "data.raw.totalCycleCount" }, 
						"_conversion_": {}
					},
					{
						"column":       { "name":  "cycleElapsedTime", "units": "s", "label": "Elapsed Cycles" },
						"_source_":     { "__field__": "data.raw.cycleElapsedTime" }, 
						"_conversion_": {}
					},
					{
						"column":       { "name":     "strain", "units": "∆", "label": "Strain" }, 
						"_source_":     { "__field__":   "data.raw.disp"},
						"_conversion_": { "_constant_": "1.0/details.gauge.value" }
					},
					{
						"column":       { "name":     "stress", "units": "MPa", "label": "Stress" },
						"_source_":     { "_lookup_": { "details.info.orientation": {"tr":"data.raw.load_missus", "lg": "data.raw.load_1kN"} } },
						"_conversion_": { "_constant_": "1.0/details.measurements.area.value" }
					},
					{
						"column":       { "name":     "step", "units": "", "label": "WaveMatrix Step" },
						"_source_":     { "__field__":    "data.raw.step" }, 
						"_conversion_": { "_constant_": "1.0/details.measurements.area.value" }
					}
				]
			},
			"trends": {
				"pre": [
		            { "name": "step",                   "label": "Step",                    "units": "Nº",  "full": "Step"                                       },
		            { "name": "totalTime",              "label": "Total Time",              "units": "s",   "full": "Total Time (s)"                             },
		            { "name": "disp",                   "label": "Displacement",            "units": "mm",  "full": "Displacement(Linear|Digital Position) (mm)" },
		            { "name": "load_1kN",               "label": "Load (1kN)",              "units": "N",   "full": "Load(Linear|Load) (N)"                      },
		            { "name": "load_missus",            "label": "Load (Missus)",           "units": "N",   "full": "Load(Linear|Load1) (N)"                     },
		            { "name": "elapsedCycles",          "label": "Elapsed Cycles",          "units": "Nº",  "full": "Elapsed Cycles"                             },
		            { "name": "cycleElapsedTime",       "label": "Cycle Elapsed Time ",     "units": "s",   "full": "Cycle Elapsed Time (s)"                     },
		            { "name": "totalCycleCount",        "label": "Total Cycle Count",       "units": "Nº",  "full": "Total Cycle Count(Linear Waveform)"         }
				],
				"norm": [
					{
						"column":     { "name":  "totalTime", "units": "s", "label": "Total Time" },
						"_source_":     { "__field__": "data.raw.totalTime" }, 
						"_conversion_": {}
					},
					{
						"column":     { "name":     "step", "units": "", "label": "WaveMatrix Step" },
						"_source_":     { "__field__":    "data.raw.step" }, 
						"_conversion_": { "_constant_": "1.0/details.measurements.area.value" }
					}
				]
			}
		}
	}
}
